{
  "name": "@sciencelogic/revis-network",
  "version": "1.1.0",
  "description": "Network visualization engine with demo build on react-transform-boilerplate template.",
  "author": "jonmodell<jon@jonmodell.com>",
  "license": "MIT",
  "repository": "/ScienceLogic/revis-network",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ramda": "^0.27.0",
    "react-resize-detector": "^6.6.4",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-a11y": "^6.0.27",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-storysource": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/react": "^10.0.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0",
    "core-js": "3.9.1",
    "cross-env": "^7.0.3",
    "d3-force": "^2.1.1",
    "dagre": "^0.8.5",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.1.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-is": "^17.0.2",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.2.1",
    "typescript": ">= 3.x",
    "webpack": "^2"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0",
    "styled-components": "^4.0.0 || ^5.0.0"
  },
  "files": [
    "dist"
  ]
}
